{
    "name": "cvasq/logger",
    "version": "0.1",
    "description": "Handle log in a PHP project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Vasquez",
            "email": "cvasquezr@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "psr/log": "^1.0.1 || ^2.0 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Logger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
